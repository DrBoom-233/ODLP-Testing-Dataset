@media only screen and (max-width: 1023px) {
  header.desktop { overflow: hidden; position: relative; }
  header.desktop::after { animation: 0.6s ease-in-out 0s infinite alternate=
 none running dissolve; inset: 0px; content: ""; display: block; position: =
absolute; z-index: 1000; }
}

@media only screen and (min-width: 1024px) {
  header.mobile { overflow: hidden; position: relative; }
  header.mobile::after { animation: 0.6s ease-in-out 0s infinite alternate =
none running dissolve; inset: 0px; content: ""; display: block; position: a=
bsolute; z-index: 1000; }
}

.white, .white-link { color: rgb(255, 255, 255) !important; }

.white-link { border-bottom-color: rgb(255, 255, 255) !important; }

.white-link:hover { border-bottom-color: rgb(255, 255, 255); }

.font-small { font-weight: 300; }

.undecorated-link { border-bottom: none; text-decoration: none; }

.hr-divider { border-top: none; border-right: none; border-left: none; bord=
er-image: initial; border-bottom: 1px solid silver; margin: 0px; }

.full-height { height: 100%; }

.full-width { width: 100%; }

.links-wrap { gap: 32px; }

.vertical-separator > :not(:last-child) { position: relative; }

.vertical-separator > :not(:last-child)::before { content: ""; display: blo=
ck; pointer-events: none; position: absolute; right: -16px; top: 50%; trans=
form: translate(50%, -50%); }

.link-rail-vertical-bar, .vertical-separator > :not(:last-child)::before { =
background-color: rgb(214, 214, 214); height: 16px; width: 1px; }

.visibility-hidden { clip: rect(0px, 0px, 0px, 0px); border: 0px; height: 1=
px; margin: -1px; overflow: hidden; padding: 0px; position: absolute; width=
: 1px; }

.relative { position: relative; }

.align-items-start { align-items: flex-start; }

.grid-x.nowrap, .grid-y.nowrap { flex-wrap: nowrap; }

.link-rail-item { border: none; cursor: pointer; display: inline-block; pos=
ition: relative; text-decoration: none; }

